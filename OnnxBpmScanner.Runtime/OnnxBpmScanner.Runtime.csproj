  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\OnnxBpmScanner.Core\OnnxBpmScanner.Core.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Ressources\Audios\Hit It [100].mp3">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="Ressources\Audios\Steve Monite - Only You [117,89].mp3">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="Ressources\Audios\Tappa Zukie &amp; Horace Andy - Natty Dread A Weh She Want [140].mp3">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
