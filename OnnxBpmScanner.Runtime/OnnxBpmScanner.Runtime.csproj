  </PropertyGroup>

  <ItemGroup>
    <None Remove="Ressources\beat_this.onnx" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Ressources\beat_this.onnx">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
